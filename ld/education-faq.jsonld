  "mainEntity": [
    {
      "@type": "Question",
      "name": "Wie schützt SafeShare die Privatsphäre?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "SafeShare verarbeitet Links ausschließlich im Browser. Es gibt keine Accounts, keine Server-Logs und keine Tracking-Cookies."
      }
    },
    {
      "@type": "Question",
      "name": "Eignet sich SafeShare für Schulen und NGOs?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Ja. Lehrkräfte, Hochschulen und NGOs können Links vor dem Teilen bereinigen. Optional als PWA installierbar und offline nutzbar."
      }
    },
    {
      "@type": "Question",
      "name": "Braucht man Adminrechte oder IT?",
      "acceptedAnswer": {
        "@type": "Answer",
        "text": "Nein. SafeShare ist eine Web-App und läuft in gängigen Browsern ohne Installation. PWA-Installation ist optional."
      }
    }
  ]
}
